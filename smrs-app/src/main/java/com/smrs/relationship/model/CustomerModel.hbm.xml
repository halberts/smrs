<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.smrs.relationship.model">
	<class table="customer" name="CustomerModel">
		<id  name="id" column="id" type="long">
			<generator class="native" />
		</id>
		<property name="name" column="name"/>
		<property name="code" column="code"/>
		<property name="type" column="type"/>
		<property name="level" column="level"/>
		<property name="province" column="province"/>
		<property name="city" column="city"/>   		
		<property name="zone" column="zone"/>	
		<property name="address" column="address"/>
		<property name="contact" column="contact"/>
		<property name="tel" column="tel"/>
		<property name="mobile" column="mobile"/>
		<property name="regionId" column="region_id"/>
		
		
		<property name="status" column="status"/>
		<property name="creator" column="creator"/>
		<property name="creationDate" column="creation_date"/>
		<property name="modifiedBy" column="modified_by"/>
		<property name="lastModifyDate" column="last_modify_date"/>	
		

	</class>
</hibernate-mapping>
